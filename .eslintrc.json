{
    "plugins": ["json"],
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "double"],
        "json/*": ["error", { "allowComments": true }]
    }
}
